{
    "parser": "@babel/eslint-parser",
    "extends": ["standard", "react-app"],
    "rules": {
        "semi": [2, "always"],
      "no-trailing-spaces": ["error", { "skipBlankLines": true } ],
      "no-return-assign": 0,
      "eol-last": 0
    },
    "parserOptions":{
        "requireConfigFile": false
    }
  }