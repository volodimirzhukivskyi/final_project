{
  "name": "final_project",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://volodimirzhukivskyi.github.io/final_project/",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "babel-plugin-macros": "^3.1.0",
    "classnames": "^2.3.1",
    "standard": "^17.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "npm-run-all -p watch-styles-sass start-js",
    "build": "npm run stylelint-build && react-scripts build",
    "start-js": "react-scripts start",
    "stylelint-fix-css": "stylelint --fix 'src/**/*.css'",
    "test": "react-scripts test",
    "stylelint-dev": "stylelint 'src/**/*.css' --cache --cache-location './.stylelintcache'; exit 0",
    "stylelint-build": "stylelint 'src/**/*.css'",
    "watch-styles-sass": "npm-watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "watch": {
    "stylelint-dev": {
      "patterns": [
        "/src/**/*.css"
      ],
      "extensions": "css"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "date-fns": "^2.29.3",
    "formik": "^2.2.9",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "react-svg": "^15.1.10",
    "redux": "^4.2.0",
    "stylelint": "^14.13.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-order": "^5.0.0"
  }
}
